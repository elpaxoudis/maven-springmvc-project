  <groupId>me.test.springmvc</groupId>
  <artifactId>maven-springmvc-project</artifactId>
  <packaging>war</packaging>
  <version>1.0-SNAPSHOT</version>
  <name>maven-springmvc-project Maven Webapp</name>

